{
  "name": "@fczbkk/webpack-example",
  "version": "1.0.0",
  "devDependencies": {
    "cross-env": "^7.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "prod-param": "webpack --config ./webpack.config.js --mode=production",
    "dev-param": "webpack --config ./webpack.config.js --mode=development",
    "prod-env": "cross-env NODE_ENV=production npm run build",
    "dev-env": "cross-env NODE_ENV=development npm run build"
  }
}
